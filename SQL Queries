All SQL queries related to this project 

create database bankmanagementsystem;

show databases;

use bankmanagementsystem;

create table signup(formno varchar(20), name varchar(20), father_name varchar(20), dob varchar(20), gender varchar(20), email varchar(30), marital_status varchar(20), address varchar(40), city varchar(25), pincode varchar(20), state varchar(25));

show tables;

SET SQL_SAFE_UPDATES = 0;

select * from signup;

create table signuptwo(formno varchar(20), religion varchar(20), category varchar(20), income varchar(20), education varchar(20), occupation varchar(20), pan varchar(20), adhaar varchar(20), seniorcitizen varchar(20), existingaccount varchar(20));

delete from signup where name = "bilal";

delete from signup where name = "bilal";

select * from signuptwo;

create table signupthree(formno varchar(20), accountType varchar(40), cardnumber varchar(25), pin varchar(10), facilities varchar(100));

delete from signuptwo where income = "Null";

select * from signupthree;

delete from signupthree where pin = 2079;

create table login(formno varchar(20), cardnumber varchar(25), pin varchar(10));  

select * from login;

delete from login where pin = 2079;

delete from login;

ROLLBACK;

create table bank(pin varchar(15), date varchar(50), type varchar(20), amount varchar(20)); 

select * from bank;

delete from bank where pin = 2079;

drop table signupthree;

drop table login;

drop table bank;
